{
  "name": "viact",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:host": "vite --port 3000 --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "cz",
    "lint": "eslint --cache --cache-location cache/.eslintcache --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint --cache --cache-location cache/.stylelintcache --ignore-path .gitignore \"**/*.css\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:all": "npm run lint && npm run lint:style",
    "lint:all:fix": "npm run lint -- --fix && npm run lint:style -- --fix",
    "format": "prettier -uw --cache --ignore-path .gitignore .",
    "test": "cypress open",
    "test:components": "cypress run --component",
    "test:e2e": "cypress run --e2e",
    "test:all": "npm run test:components && npm run test:e2e"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-plugin-twin": "^1.0.2",
    "commitizen": "^4.2.5",
    "cypress": "^10.3.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.10.3",
    "vite": "^3.0.0"
  }
}
