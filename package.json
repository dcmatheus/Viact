{
  "name": "viact",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:host": "vite --port 3000 --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "cz",
    "lint": "eslint --cache --cache-location cache/.eslintcache --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint --cache --cache-location cache/.stylelintcache --ignore-path .gitignore \"**/*.css\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:all": "npm run lint && npm run lint:style",
    "lint:all:fix": "npm run lint -- --fix && npm run lint:style -- --fix",
    "format": "prettier -uw --cache --ignore-path .gitignore .",
    "test": "cypress open",
    "test:components": "cypress run --component",
    "test:e2e": "cypress run --e2e",
    "test:all": "npm run test:components && npm run test:e2e"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.118",
    "@svgr/core": "^6.4.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.0.0",
    "commitizen": "^4.2.5",
    "cypress": "^10.3.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.11",
    "vite": "^3.0.0",
    "vite-plugin-windicss": "^1.8.8",
    "vite-tsconfig-paths": "^3.5.1",
    "windicss": "^3.5.6"
  }
}
